generator client {
  provider        = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Account {
  id        String   @id @default(uuid())
  name      String
  balance   Float    @default(0)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model balance_history {
  id        String   @id @default(uuid())
  date      DateTime
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
